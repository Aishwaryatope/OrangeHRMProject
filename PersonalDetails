package orangehrmproject;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.IOException;
import java.util.concurrent.TimeUnit;

import org.apache.poi.xssf.usermodel.XSSFSheet;
import org.apache.poi.xssf.usermodel.XSSFWorkbook;
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.testng.annotations.AfterTest;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.Test;

@Test
public class Myinfopage {
	 public WebDriver driver;
	    
	    @BeforeTest
	    public void System() {
	    	System.setProperty("webdriver.chrome.driver", "C:\\Users\\Sumit\\Downloads\\chromedriver-win64\\chromedriver-win64\\chromedriver.exe");
	    	 driver = new ChromeDriver();
	    	 driver.manage().timeouts().implicitlyWait(10, TimeUnit.SECONDS);
	    	 
	    }
	    @Test(priority=1,groups="loginpage")
	    
	    public void loginpageTest() {
	    	driver.get( "https://opensource-demo.orangehrmlive.com/web/index.php/auth/login");
	    	driver.manage().window().maximize();
	    	FileInputStream d = null;
	    	try {
			
				d = new FileInputStream("C:\\Users\\Sumit\\Desktop\\orange.xlsx");
			} catch (FileNotFoundException e) {
				
			
			}
	    	
				XSSFWorkbook wb = null;
			try {	
					wb = new XSSFWorkbook(d);
			} catch (IOException e) {
				
				
			}
	    	XSSFSheet ws=wb.getSheet("Sheet1");
	    	
	    	//Username
	    	String name=ws.getRow(1).getCell(1).getStringCellValue();
	    	String username=ws.getRow(1).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(name)).sendKeys(username);
	    	
	    	//Password
	    	String pwd=ws.getRow(2).getCell(1).getStringCellValue();
	    	String pwd1=ws.getRow(2).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(pwd)).sendKeys(pwd1);
	    	
	    	//Login Button
	    	String btn=ws.getRow(3).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(btn)).click();
	    	try {
				Thread.sleep(3000);
			} catch (InterruptedException e) {
				
				e.printStackTrace();
			}
	    	
	    }
	   @Test(priority = 2,groups ="personaldetails") 
	   
	  public void Personaldetails() {
		  FileInputStream d = null;
		
	    	try {
			
				d = new FileInputStream("C:\\Users\\Sumit\\Desktop\\orange.xlsx");
			} catch (FileNotFoundException e) {
				
			
			}
	    	
				XSSFWorkbook wb = null;
			try {	
					wb = new XSSFWorkbook(d);
			} catch (IOException e) {
				
				
			}
	    	XSSFSheet ws=wb.getSheet("Sheet1");
	    	
	    //My Info page
	    	String myinfo=ws.getRow(5).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(myinfo)).click();	 
	    	driver.manage().timeouts().implicitlyWait(1000, TimeUnit.SECONDS);
	    	
	   	//First Name
	    	String firstname=ws.getRow(7).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(firstname)).click();
	    	String name=ws.getRow(7).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(firstname)).sendKeys(name);
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	   //Middle Name 	
	    	String middlename=ws.getRow(8).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(middlename)).click();
	    	String m_n=ws.getRow(8).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(middlename)).sendKeys(m_n);
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //Last Name
	    	String lastname = ws.getRow(9).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(lastname)).click();
	    	String l_n = ws.getRow(9).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(lastname)).sendKeys(l_n);
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //Nick Name	
	    	String nickname = ws.getRow(10).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(nickname)).click();
	    	String n_e = ws.getRow(10).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(nickname)).sendKeys(n_e);
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //Empolyee ID
	    	String employeeid = ws.getRow(11).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(employeeid)).click();
	    	String employee = ws.getRow(11).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(employeeid)).sendKeys(employee);	
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //Other ID
	    	String otherid = ws.getRow(12).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(otherid)).click();
	    	String other = ws.getRow(12).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(otherid)).sendKeys(other);		
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //Driver Lisence Number
	    	String driver_lisence_number = ws.getRow(13).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(driver_lisence_number)).click();
	    	String driver_lisence= ws.getRow(13).getCell(2).getStringCellValue();
	         driver.findElement(By.xpath(driver_lisence_number)).sendKeys(driver_lisence);
	         driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
		    	 
	    //ssn Number	
	    	String ssn_number = ws.getRow(15).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(ssn_number)).click();
	    	String ssn = ws.getRow(15).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(ssn_number)).sendKeys(ssn);
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //sin Number
	    	String sinnumber=ws.getRow(16).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(sinnumber)).click();
	    	String sin=ws.getRow(16).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(sinnumber)).sendKeys(sin);
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //Nationality	    	
	    	String nationality=ws.getRow(17).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(nationality)).click();
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //martial status
	    	String martialstatus=ws.getRow(18).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(martialstatus)).click();
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //date of birth
	    	String date_birth=ws.getRow(19).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(date_birth)).click();
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //Gender	
	    	String gender2=ws.getRow(20).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(gender2)).click();
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //military service	
	    	String military_service=ws.getRow(21).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(military_service)).click();
	    	String military=ws.getRow(21).getCell(2).getStringCellValue();
	    	driver.findElement(By.xpath(military_service)).sendKeys(military);
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	    //Save	
	    	String personality_details_save=ws.getRow(22).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(personality_details_save)).click();
	    	driver.manage().timeouts().implicitlyWait(1000, TimeUnit.SECONDS);
	    	
	    //	Blood Type
	    	String blood_type=ws.getRow(23).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(blood_type)).click();
	    	driver.manage().timeouts().implicitlyWait(2000, TimeUnit.SECONDS);
	    	
	    //save blood type	
	    	String save_blood_type_save=ws.getRow(24).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(save_blood_type_save)).click();
	    	driver.manage().timeouts().implicitlyWait(1000, TimeUnit.SECONDS);
	    	
	    //Add attachment	
	    	String Add_attachment=ws.getRow(25).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(Add_attachment)).click();
	    	driver.manage().timeouts().implicitlyWait(2000, TimeUnit.SECONDS);
	    	
	    //Browser attachment	
	    	String Browser_attachment=ws.getRow(26).getCell(1).getStringCellValue();
	    	driver.findElement(By.xpath(Browser_attachment)).click();
	    	driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);
	    	
	  }
	    	 @AfterTest
			 public void exit() {
			
			 driver.close();	
	    	
	    	
	    	 
	    
	    	
	    	
	    	
	    	
	    	
	  }
	    	
	    	
}
	    
	    
	    
	    

